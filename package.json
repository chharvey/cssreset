{
  "name": "resetcss",
  "version": "1.0.3",
  "description": "Yet another CSS reset stylesheet.",
  "main": "reset.css",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lessc:all": "lessc reset.less reset.css",
    "minify": "cleancss reset.css -o reset.min.css",
    "unbuild": "rm reset.css reset.min.css",
    "build": "npm run lessc:all  &&  npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chharvey/resetcss.git"
  },
  "keywords": [
    "css",
    "reset",
    "browser"
  ],
  "author": "Chris Harvey <chrisharvey2pi@gmail.com> (https://chharvey.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chharvey/resetcss/issues"
  },
  "homepage": "https://github.com/chharvey/resetcss#readme",
  "devDependencies": {
    "clean-css": "^3.4.17",
    "less": "^2.7.1"
  }
}
